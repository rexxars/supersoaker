{
  "name": "supersoaker",
  "description": "Add custom npm registry + user credentials to npmrc based on environment variables or CLI options",
  "version": "1.0.1",
  "keywords": [
    "npm",
    "adduser",
    "registry"
  ],
  "main": "src/supersoaker.js",
  "bin" : {
    "supersoaker" : "./src/supersoaker.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:rexxars/supersoaker.git"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  },
  "author": "Espen Hovlandsdal <espen@hovlandsdal.com>",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^0.21.2",
    "eslint-config-vaffel": "^1.0.0"
  },
  "dependencies": {
    "npm": "^2.10.1",
    "yargs": "^3.9.1"
  }
}
